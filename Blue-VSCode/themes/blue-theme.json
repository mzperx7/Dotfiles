		},
		{
			"scope": "strong",
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
                "comment",
                "punctuation.definition.comment",
                "punctuation.definition.comment.begin.c",
                "punctuation.definition.comment.end.c",
