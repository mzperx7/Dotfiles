                "entity.other.inherited-class",
				"entity.name.function.preprocessor.c"
            ],
			"settings": {
				"foreground": "#b3bcc5"
			}
		},
		{
			"scope": [
                "invalid.deprecated",
                "invalid.illegal",
				"invalid.illegal.placeholder"
            ],
