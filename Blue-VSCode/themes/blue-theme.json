                "constant.language",
                "constant.regexp"
            ],
			"settings": {
				"foreground": "#6c7d8e"
			}
		},
        {
        "scope": [
		        "constant.character",
                "constant.character.escape",
                "constant.numeric",
                "keyword.constant"
            ],
			"settings": {
				"foreground": "#f5deb3"
			}
		},	
        {
			"scope": [
			    "entity.name.type",
				"entity.name.class",
                "entity.name.tag",
				"entity.name.type.class",
				"entity.name.type.enum",
                "entity.name.function.c"			
			],
			"settings": {
				"foreground": "#6c7d8e"
			}
		},
		{
			"scope": [
