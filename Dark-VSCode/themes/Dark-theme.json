            ],
			"settings": {
				"foreground": "#b3bcc5"
			}
		},
		{
			"scope": [
                "invalid.deprecated",
                "invalid.illegal"
            ],
			"settings": {
				"foreground": "#8e1201"
			}
		},
		{
			"scope": "invalid.illegal.placeholder",
